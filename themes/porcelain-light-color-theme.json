    "terminal.ansiBrightGreen": "#4d8e3e",
    "terminal.ansiBrightMagenta": "#8c57d1",
    "terminal.ansiBrightRed": "#b12f4b",
    "terminal.ansiBrightWhite": "#ffffff",
    "terminal.ansiBrightYellow": "#bd883d",
