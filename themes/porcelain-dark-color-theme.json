    "editorGutter.addedBackground": "#89b4fa",
    "editorGutter.deletedBackground": "#f38ba8",
    "editorBracketHighlight.foreground1": "#7dc4e4",
    "editorBracketHighlight.foreground2": "#89b4fa",
    "editorBracketHighlight.foreground3": "#f9e2af",
    "editorBracketHighlight.foreground4": "#c6a0f6",
    "editorBracketHighlight.foreground5": "#eed49f",
    "editorBracketHighlight.foreground6": "#89b4fa",

    "debugToolBar.background": "#0f172b",

    "editor.stackFrameHighlightBackground": "#a707",
    "editor.focusedStackFrameHighlightBackground": "#b808",

    "peekViewEditor.background": "#0f172b",
    "peekViewResult.background": "#0f172b",

    "settings.headerForeground": "#ffffff",
    "settings.modifiedItemIndicator": "#89b4fa",

    "problemsErrorIcon.foreground": "#ed8796",
    "problemsWarningIcon.foreground": "#eed49f",
    "problemsInfoIcon.foreground": "#8aadf4",
    "editorError.foreground": "#ed8796",
    "editorWarning.foreground": "#eed49f",
    "editorInfo.foreground": "#8aadf4",
    "editorHint.foreground": "#89dceb",

    "editorGutter.commentRangeForeground": "#ffffff50",
    "editorGutter.foldingControlForeground": "#ffffff90",
    "editorInlayHint.foreground": "#666666",
    "editorInlayHint.background": "#151f33",
    "editorStickyScroll.background": "#151f33",
    "editorStickyScrollHover.background": "#151f33",
    "menu.separatorBackground": "#1d293d"
  },
  "semanticTokenColors": {
    "class": "#f9e2af",
    "interface": "#f9e2af",
    "struct": "#f9e2af",
    "type": "#f9e2af",
    "function": "#89b4fa",
    "method": "#89b4fa",
    "comment": "#62748e",
    "string": "#a6e3a1",
    "keyword": "#cba6f7",
    "number": "#fab387",
    "operator": "#74c7ec",
    "regexp": "#74c7ec",
    // e.g. `__init__` in python
    "magicFunction": "#cba6f7",
    // Reset defaults
    "variable": "#e2e8f0",
    "property": "#e2e8f0",
    "namespace": "#e2e8f0",
    // e.g. `Math` or `window` in javascript
    "variable.defaultLibrary": "#f38ba8",
    // `self` parameter in python
    "selfParameter": {
      "foreground": "#f38ba8",
      "italic": true
    },
    // e.g. `Partial` in typescript
    // "type.defaultLibrary": "#fab387",
    // e.g. `str` in python
    // "class.typeHint.builtin": "#fab387"
  },
  "semanticHighlighting": true,
  "tokenColors": [
    {
      "name": "Global",
      "settings": {
        "foreground": "#e2e8f0"
      }
    },
    {
      "name": "Variables",
      "scope": [
        // Variables (e.g. `variable` in `variable = value`)
        // Useful for regexp named group in javascript
        "variable"
      ],
      "settings": {
        "foreground": "#e2e8f0"
      }
    },
    {
      "name": "Comments",
      "scope": [
        // Comment blocks
        "comment",
        // `//` in `// Comments`
        "punctuation.definition.comment"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#62748e"
      }
    },
    {
      "name": "Syntax Noises",
      "scope": [
        // e.g. `{}` in javascript function blocks
        "punctuation.definition",
        // Commas (,)
        "punctuation.separator.comma",
        // Semicolons (;)
        "punctuation.terminator",
        // `{}` in python f-strings
        "constant.character.format.placeholder"
      ],
      "settings": {
        "foreground": "#62748e"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        // Keywords (e.g. `import` in javascript, `g` in regexp, `!important` in css)
        "keyword",
        // e.g. `@` in `@keyframes`
        "punctuation.definition.keyword",
        // e.g. `function` in javascript declarations
        "storage.type",
        // e.g. `async` in javascript declarations
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#cba6f7"
      }
    },
    {
      "name": "Operators",
      "scope": [
        // e.g. `.` in `variable.key`
        "punctuation",
        // e.g. `=` in `variable = value`
        "keyword.operator",
        // `[]` in `variable[key]`
        "meta.brace",
      ],
      "settings": {
        "foreground": "#74c7ec"
      }
    },
    {
      "name": "Tags",
      "scope": [
        // HTML tag names
        "entity.name.tag"
      ],
      "settings": {
        "foreground": "#eba0ac"
      }
    },
    {
      "name": "Functions",
      "scope": [
        // Function declaration names, function call names, or method call names
        "entity.name.function",
        // e.g. `rgb` in css
        "support.function",
        // Python function call names
        "meta.function-call.generic",
        // Shell command
        "entity.name.command"
      ],
      "settings": {
        "foreground": "#89b4fa"
      }
    },
    {
      "name": "Literals",
      "scope": [
        // Number literals
        "constant.numeric",
        // e.g. `undefined` or `*`
        "constant.language",
        // CSS value keywords (e.g. `fixed`)
        "support.constant",
        // CSS units
        "keyword.other.unit",
        // TypeScript builtin types (e.g. `undefined`)
        "support.type.builtin"
      ],
      "settings": {
        "foreground": "#fab387"
      }
    },
    {
      "name": "Strings",
      "scope": [
        // String contents
        "string",
        // String quotes
        "punctuation.definition.string",
        // Markdown code block language
        "fenced_code.block.language"
      ],
      "settings": {
        "foreground": "#a6e3a1"
      }
    },
    {
      "name": "Classes and Types",
      "scope": [
        // "entity.name.class",
        // e.g. `Type` in `variable: Type`, or type declaration names
        "entity.name.type",
        // e.g. `Promise` in javascript
        "support.class",
        // Parent classes in javascript
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#f9e2af"
      }
    },
    {
      "name": "Keys",
      "scope": [
        // keys in json and css
        "support.type"
      ],
      "settings": {
        "foreground": "#f9e2af"
      }
    },
    {
      "name": "Special Keys",
      "scope": [
        // CSS vendor-prefixed keys (e.g. `-webkit-box-orient`)
        "support.type.vendored",
        // e.g. `void` in typescript
        // "support.type.primitive"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#f9e2af"
      }
    },
    {
      "name": "Attributes",
      "scope": [
        // HTML attribute names
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#cba6f7"
      }
    },
    {
      "name": "Deprecated Attributes",
      "scope": [
        // e.g. `align` for `p`
        "invalid.deprecated.entity.other.attribute-name"
      ],
      "settings": {
        "fontStyle": "strikethrough",
        "foreground": "#cba6f7"
      }
    },
    {
      "name": "Special Variables",
      "scope": [
        // e.g. `this` in javascript, `self` in python
        "variable.language",
        // CSS vendor-prefixed values (e.g. `-webkit-box`)
        "support.constant.vendored"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#f38ba8"
      }
    },
    {
      "name": "Special Syntax",
      "scope": [
        // Regular expressions
        "string.regexp",
        // Escape characters
        "constant.character.escape",
        // CSS colors
        "constant.other.color"
      ],
      "settings": {
        "foreground": "#74c7ec"
      }
    },
    {
      "name": "URL",
      "scope": [
        "*url*",
        "*link*",
        "*uri*"
      ],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Link",
      "scope": [
        "string.other.link"
      ],
      "settings": {
        "foreground": "#89b4fa"
      }
    },
    {
      "name": "Code",
      "scope": [
        "markup.inline.raw"
      ],
      "settings": {
        "foreground": "#90a1b9"
      }
    },
    {
      "name": "Bold",
      "scope": [
        "heading",
        "markup.bold"
      ],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Italic",
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Strikethrough",
      "scope": [
        "markup.strikethrough"
      ],
      "settings": {
        "fontStyle": "strikethrough"
      }
    },
    {
      "name": "Bold & Italic",
      "scope": [
        "markup.italic markup.bold",
        "markup.bold markup.italic",
        "heading markup.italic"
      ],
      "settings": {
        "fontStyle": "bold italic"
      }
    },
    {
      "name": "Bold & Strikethrough",
      "scope": [
        "markup.strikethrough markup.bold",
        "markup.bold markup.strikethrough"
      ],
      "settings": {
        "fontStyle": "bold strikethrough"
      }
    },
    {
      "name": "Italic & Strikethrough",
      "scope": [
        "markup.strikethrough markup.italic",
        "markup.italic markup.strikethrough"
      ],
      "settings": {
        "fontStyle": "italic strikethrough"
      }
    },
    {
      "name": "Bold, Italic & Strikethrough",
      "scope": [
        "markup.italic markup.bold markup.strikethrough",
        "markup.italic markup.strikethrough markup.bold",
        "markup.strikethrough markup.bold markup.italic",
        "markup.strikethrough markup.italic markup.bold",
        "markup.bold markup.italic markup.strikethrough",
        "markup.bold markup.strikethrough markup.italic",
        "heading markup.italic markup.strikethrough",
        "heading markup.strikethrough markup.italic"
      ],
      "settings": {
        "fontStyle": "bold italic strikethrough"
      }
    }
  ]
}
